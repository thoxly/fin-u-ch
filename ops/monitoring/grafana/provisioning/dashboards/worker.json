{
  "dashboard": {
    "title": "Worker Metrics",
    "tags": ["worker", "jobs"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Job Execution Frequency",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(worker_job_total[5m])",
            "legendFormat": "{{job_name}} - {{status}}"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Job Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(worker_job_duration_seconds_bucket[5m]))",
            "legendFormat": "{{job_name}} - p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(worker_job_duration_seconds_bucket[5m]))",
            "legendFormat": "{{job_name}} - p50"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 3,
        "title": "Job Success/Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(worker_job_total{status=\"success\"}[5m])",
            "legendFormat": "{{job_name}} - Success"
          },
          {
            "expr": "rate(worker_job_total{status=\"error\"}[5m])",
            "legendFormat": "{{job_name}} - Error"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 4,
        "title": "Last Successful Execution Time",
        "type": "graph",
        "targets": [
          {
            "expr": "time() - worker_job_last_success_timestamp",
            "legendFormat": "{{job_name}} (seconds ago)"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1
  }
}
